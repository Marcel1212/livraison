<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;

/**
 * @property float $id_rapports_visites
 * @property float $id_visites
 * @property string $contenu
 * @property boolean $flag_rapports_visites
 * @property string $date_rapport
 * @property string $created_at
 * @property string $updated_at
 * @property Visite $visite
 */
class RapportsVisites extends Model
{
    /**
     * The primary key for the model.
     * 
     * @var string
     */
    protected $primaryKey = 'id_rapports_visites';

    /**
     * The "type" of the auto-incrementing ID.
     * 
     * @var string
     */
    protected $keyType = 'float';

    /**
     * @var array
     */
    protected $fillable = ['id_visites', 'contenu', 'flag_rapports_visites', 'date_rapport', 'created_at', 'updated_at'];

    /**
     * @return \Illuminate\Database\Eloquent\Relations\BelongsTo
     */
    public function visite()
    {
        return $this->belongsTo('App\Models\Visite', 'id_visites', 'id_visites');
    }
}
